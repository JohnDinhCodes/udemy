<% include partials/header %>
<div class="ui main container">
    <h1>Index Page</h1>

    <% for (let blog of blogs) { %>
    <div>
        <h2><%= blog.title %></h2>
        <img src="<%= blog.image %>">
        <span><%= blog.created %></span>
        <p><%= blog.body.substring(0, 100) %>...</p>
        <a href="/blogs/<%=blog._id %>">Read More</a>
    </div>
    <% } %>
</div>


<% include partials/footer %>